<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserInfoActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="23dp"
        android:layout_height="44dp"
        android:fontFamily="@font/nanumbarunb"
        android:text="-"
        android:textSize="33sp"
        app:layout_constraintStart_toEndOf="@+id/userinfo_birth"
        app:layout_constraintTop_toBottomOf="@+id/birthcode" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="104dp"
        android:layout_height="43dp"
        android:fontFamily="@font/nanumbarunb"
        android:text="******"
        android:textSize="33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toEndOf="@+id/userinfo_sex"
        app:layout_constraintTop_toBottomOf="@+id/birthcode" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="326dp"
        android:layout_height="54dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/jalnan"
        android:text="안녕하세요!"
        android:textColor="@color/mainBlack"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userinfo_logo1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="47dp"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="41dp"
        android:fontFamily="@font/nanumbarunb"
        android:text="@string/userinfo"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/userinfo_logo1"
        android:layout_width="335dp"
        android:layout_height="0dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="536dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toTopOf="@+id/userinfo_next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/swudical_logo" />

    <TextView
        android:id="@+id/userinfo_name_text"
        android:layout_width="69dp"
        android:layout_height="44dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/nanumbarunb"
        android:gravity="left"
        android:text="이름"
        android:textColor="@color/mainPurple"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" />

    <EditText
        android:id="@+id/userinfo_name"
        android:layout_width="164dp"
        android:layout_height="44dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:hint="김슈니"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userinfo_name_text" />

    <TextView
        android:id="@+id/birthcode"
        android:layout_width="172dp"
        android:layout_height="41dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/nanumbarunb"
        android:gravity="center|center_horizontal|left"
        android:text="주민등록번호"
        android:textColor="@color/mainPurple"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userinfo_name" />

    <EditText
        android:id="@+id/userinfo_birth"
        android:layout_width="135dp"
        android:layout_height="44dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:gravity="left"
        android:inputType="number"
        android:maxLength="6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/birthcode"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userinfo_email_text"
        android:layout_width="86dp"
        android:layout_height="44dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/nanumbarunb"
        android:gravity="left"
        android:text="이메일"
        android:textColor="@color/mainPurple"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/userinfo_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:background="#EAE3F6"
        android:text="NEXT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.869"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userinfo_logo1" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="300dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userinfo_email_text" />

    <EditText
        android:id="@+id/userinfo_sex"
        android:layout_width="35dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="number"
        android:maxLength="1"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/birthcode" />


</androidx.constraintlayout.widget.ConstraintLayout>